{
  "tasks": [
    {
      "type": "shell",
      "label": "g++.exe build active file",
      "command": "C:MinGW64\bing++.exe",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileDirname}${fileBasenameNoExtension}.exe"
      ],
      "options": {
        "cwd": "C:MinGW64\bin"
      }
    },
    {
      "type": "shell",
      "label": "g++ build & run active file",
      "command": "C:MinGW64\bing++.exe",
      "args": ["${file}", "-o", "${fileDirname}${fileBasenameNoExtension}.exe"],
      "options": {
        "cwd": "C:MinGW64\bin"
      }
    }
  ],
  "version": "2.0.0"
}
